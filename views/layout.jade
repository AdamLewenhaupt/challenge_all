doctype 5
html
  head
    title= title
    link(rel='stylesheet', href='/stylesheets/style.css')
    block head
  body
    #content
        block content
    #ssv 
        - ssv.forEach(function(ssv){
            - if(ssv.json)
                .ssv.json(name= ssv.name)= ssv.value
            - else
                .ssv(name= ssv.name)= ssv.value
        - })